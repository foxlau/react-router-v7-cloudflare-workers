    "react-router": "^7.6.2",
    "session-context": "^0.0.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.0",
    "@cloudflare/workers-types": "^4.20250618.0",
    "@react-router/dev": "^7.6.2",
    "@react-router/fs-routes": "^7.6.2",
    "@tailwindcss/vite": "^4.1.10",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
